{
  "gateway": {
    "mode": "local"
  },
  "agent": {
    "workspace": "/tmp/clawdbot/workspace",
    "model": {
      "primary": "anthropic/claude-opus-4-5"
    },
    "thinkingDefault": "high"
  },
  "telegram": {
    "enabled": true,
    "tokenFile": "/run/agenix/telegram-token",
    "allowFrom": ["*"],
    "groups": {
      "*": {
        "requireMention": false
      }
    }
  },
  "routing": {
    "queue": {
      "mode": "interrupt",
      "byProvider": {
        "telegram": "interrupt",
        "discord": "queue",
        "webchat": "queue"
      }
    }
  }
}
